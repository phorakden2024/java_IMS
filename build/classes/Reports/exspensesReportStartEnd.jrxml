	<query language="sql"><![CDATA[SELECT
            id,
            expense_date,
            purpose,
            quantity,
            price,
            total
        FROM public.tbl_exspense
        WHERE expense_date BETWEEN $P{REPORT_START_DATE} AND $P{REPORT_END_DATE}
        ORDER BY expense_date, id]]></query>
